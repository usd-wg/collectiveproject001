#usda 1.0

over "main"
{
    over "render"
    {
        over "fx" (
            prepend apiSchemas = ["MaterialBindingAPI"]
        )
        {
            rel material:binding = </main/render/mtl/mtl_electric_arc>
        }
    }

    over "proxy"
    {
        over "fx" (
            prepend apiSchemas = ["MaterialBindingAPI"]
        )
        {
            rel material:binding = </main/proxy/mtl/mtl_electric_arc>
        }
    }
}

